<?xml version="1.0" encoding="UTF-8"?>
<!-- Configures the Camel Context-->

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="Body2String" class="bc.payment.income.utility.Body2String" />
	
	<bean id="operation.check" class="bc.payment.income.operation.Check" />
	<bean id="operation.pay" class="bc.payment.income.operation.Pay" />
	<bean id="operation.cancel" class="bc.payment.income.operation.Cancel" />
	
</beans>
